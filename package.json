  "main": "index.js",
  "scripts": {
    "build": "snowpack build",
    "build:all": "npm-run-all clean asbuild:optimized build",
    "clean": "npm-run-all --parallel clean:wasm clean:build",
    "clean:wasm": "rimraf src/wasm",
    "clean:build": "rimraf build",
    "asbuild:untouched": "asc assembly/index.ts --target debug",
    "asbuild:optimized": "asc assembly/index.ts --target release",
    "asbuild": "npm-run-all asbuild:untouched asbuild:optimized"
  },
